<!DOCTYPE html>
<html lang="it">
<head>
    <!-- Metadata -->
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="IOT System"/>
    <title>
        {%- block title -%}IOT System{%- endblock -%}
    </title>

    <link rel="icon" href="{{ asset('build/images/sinervis/favicon.ico') }}" type="image/x-icon">

    {% block stylesheets -%}
        {{ encore_entry_link_tags('app') }}
        <style>
            #login-form h3, #login-form label {
                color: #ffffff;
            }
            .logo-name {
                margin-top: 48px;
            }
        </style>
    {% endblock %}
</head>
<body class="pace-done gray-bg">
<div class="middle-box text-center loginscreen animated fadeInDown">
    <div>
        <div>
            <h1 class="logo-name">
                <img src="{{ asset('build/images/sinervis/website_logo.png') }}" alt="logo.png" width="200px">
            </h1>
        </div>

        {% block fos_user_content %}
        {% endblock %}

    </div>
</div>

{% block javascripts %}
    {{ encore_entry_script_tags('login') }}
{% endblock %}
<!-- end:javascript -->
</body>

</html>
